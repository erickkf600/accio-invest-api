{
  "name": "accio-invest-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "implement": "node ace migration:run --force && node ace db:seed",
    "build": "node ace build --production && cd build && npm ci --production",
    "start": "cp .env build/.env && node build/server.js",
    "dev": "node ace run:execute_job && node ace serve --watch",
    "docker:background": "docker-compose up -d",
    "docker:build": "docker-compose up --build"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.3.5",
    "@types/lodash": "^4.14.200",
    "adonis-preset-ts": "^2.1.0",
    "typescript": "~4.2",
    "youch": "^2.2.2",
    "youch-terminal": "^1.1.1"
  },
  "dependencies": {
    "@adonisjs/core": "^5.1.11",
    "@adonisjs/lucid": "^15.0.3",
    "@adonisjs/repl": "^3.1.5",
    "adonis5-scheduler": "^2.1.1",
    "axios": "^1.1.3",
    "form-data": "^4.0.4",
    "lodash": "^4.17.21",
    "luxon": "^2.0.2",
    "moment": "^2.29.1",
    "mysql2": "^3.9.7",
    "pino-pretty": "^5.1.2",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19"
  }
